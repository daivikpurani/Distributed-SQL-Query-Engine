{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch SQLClient",
      "request": "launch",
      "mainClass": "com.distributed.sql.client.SQLClient",
      "projectName": "distributed-sql-engine",
      "args": "",
      "vmArgs": "-Djava.util.logging.config.file=logback.xml",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "java",
      "name": "Launch CoordinatorMain",
      "request": "launch",
      "mainClass": "com.distributed.sql.coordinator.CoordinatorMain",
      "projectName": "distributed-sql-engine",
      "args": "",
      "vmArgs": "-Djava.util.logging.config.file=logback.xml",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "java",
      "name": "Launch WorkerMain",
      "request": "launch",
      "mainClass": "com.distributed.sql.worker.WorkerMain",
      "projectName": "distributed-sql-engine",
      "args": "",
      "vmArgs": "-Djava.util.logging.config.file=logback.xml",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "java",
      "name": "Debug Current File",
      "request": "launch",
      "mainClass": "${file}",
      "projectName": "distributed-sql-engine",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "java",
      "name": "Debug Maven Test",
      "request": "launch",
      "mainClass": "",
      "projectName": "distributed-sql-engine",
      "args": "",
      "vmArgs": "",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "preLaunchTask": "maven: test"
    }
  ]
}
